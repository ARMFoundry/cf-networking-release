{
  "name": "cni-wrapper",
  "type": "cni-wrapper-plugin",
  "cniVersion": "0.2.0",
  "datastore": "<%= p('cni-flannel.cni_datastore_path') %>",
  "iptables_lock_file": "<%= p('cni-flannel.cni_iptables_lock_file') %>",
  "overlay_network": "<%= p('garden-cni.network') %>",
  "delegate": {
    "name": "cni-flannel",
    "type": "flannel",
    "subnetFile": "/var/vcap/data/flannel/subnet.env",
    "dataDir": "/var/vcap/data/flannel/data",
    "delegate": {
      "bridge": "cni-flannel0",
      "isDefaultGateway": true,
      "ipMasq": false
     }
  }
}
